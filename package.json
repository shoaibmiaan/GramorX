    "next-themes": "^0.3.0",
    "next-pwa": "^5.6.0",
    "openai": "^5.12.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-markdown": "^9.0.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "twilio": "^5.8.0",
    "zod": "^3.25.76",
    "remark-gfm": "^3.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.19.10",
    "@types/react": "18.2.66",
